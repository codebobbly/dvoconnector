{
  "name": "rgu/dvoconnector",
  "version": "1.0.0",
  "type": "typo3-cms-extension",
  "description": "DVO Connector",
  "keywords": [
    "TYPO3",
    "extension",
    "dvo",
	"connector"
  ],
  "homepage": "https://github.com/codebobbly/dvoconnector",
  "authors": [
    {
      "name": "codebobbly",
      "role": "Developer",
      "homepage": "https://github.com/codebobbly"
    }
  ],
  "require": {
    "typo3/cms-core": ">=7.6.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.0",
    "nimut/testing-framework": "^1.0 || ^2.0 || ^3.0"
  },
  "autoload": {
    "psr-4": {
      "RGU\\Dvoconnector\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RGU\\Dvoconnector\\Tests\\": "Tests"
    }
  },
  "replace": {
    "dvoconnector": "self.version",
    "typo3-ter/dvoconnector": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/Web/typo3conf/ext/",
      "[ -L .Build/Web/typo3conf/ext/dvoconnector ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/dvoconnector"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
